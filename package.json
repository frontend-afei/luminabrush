{
  "private": true,
  "scripts": {
    "build": "DATABASE_URL=$MIGRATION_DATABASE_URL pnpm dlx prisma generate && pnpm dlx prisma migrate deploy && nuxt build",
    "dev": "run-p start:*",
    "start": "run-p start:*",
    "start:prisma-generate": "prisma generate",
    "start:dev": "nuxt dev",
    "generate": "nuxt generate",
    "postinstall": "run-p postinstall:*",
    "postinstall:prisma-migrate": "source .env && DATABASE_URL=$MIGRATION_DATABASE_URL pnpm dlx prisma migrate dev",
    "postinstall:prepare": "nuxt prepare",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "nuxt typecheck",
    "preview": "NODE_ENV=production node .output/server/index.mjs"
  },
  "dependencies": {
    "@nuxt/content": "^2.4.3",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/google-fonts": "3.0.0-1",
    "@nuxtjs/i18n": "^8.0.0-beta.10",
    "@nuxtjs/tailwindcss": "^6.4.0",
    "@prisma/client": "^4.10.1",
    "@sidebase/nuxt-auth": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@trpc/client": "^10.12.0",
    "@trpc/server": "^10.12.0",
    "next-auth": "^4.19.2",
    "prisma": "^4.10.1",
    "superjson": "^1.12.2",
    "tailwindcss-themer": "^2.0.3",
    "trpc-nuxt": "^0.7.0",
    "typescript": "^4.9.5",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^18.13.0",
    "eslint": "^8.34.0",
    "lemonsqueezy.ts": "^0.1.6",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.2.2",
    "vue-tsc": "^1.0.24"
  }
}
