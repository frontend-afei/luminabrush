{
	"name": "supastarter-nuxt",
	"version": "0.0.1",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/**/*"
	],
	"engines": {
		"node": ">=v18.16.0"
	},
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"start": "pnpm run dev",
		"lint": "turbo run lint",
		"lint:fix": "turbo run lint:fix"
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"turbo": "latest"
	},
	"packageManager": "pnpm@8.6.10"
}
